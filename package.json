        "@types/mocha": "^9.1.1",
        "@types/node": "^17.0.35",
        "chai": "^4.3.6",
        "chokidar-cli": "^3.0.0",
        "mocha": "^10.0.0",
        "ts-node": "^10.8.0",
        "typescript": "^4.7.2"
